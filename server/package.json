{
  "name": "invoice-ocr-server",
  "version": "1.0.0",
  "main": "dist/server/src/app.js",
  "_moduleAliases": {
    "@": "dist/server/src"
  },
  "scripts": {
    "start": "node -r module-alias/register -r dotenv/config .",
    "dev": "ts-node-dev --respawn -r dotenv/config -r tsconfig-paths/register src/app.ts",
    "build": "tsc",
    "init-db": "ts-node --project ./tsconfig.json ./scripts/initDatabase.ts",
    "test-reminder": "ts-node -r dotenv/config -r tsconfig-paths/register src/scripts/test-reminder.ts",
    "test-email": "ts-node -r dotenv/config -r tsconfig-paths/register src/scripts/test-email-digest.ts",
    "start-reminder": "ts-node -r dotenv/config -r tsconfig-paths/register src/server.ts"
  },
  "dependencies": {
    "@google-cloud/vision": "^4.0.2",
    "@supabase/supabase-js": "^2.50.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "camelcase-keys": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint-import-resolver-typescript": "^4.4.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "line-notify-nodejs": "^1.0.1",
    "module-alias": "^2.2.3",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "snakecase-keys": "^5.4.5",
    "uuid": "^13.0.0",
    "zod": "^3.25.46"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^7.0.1",
    "@types/uuid": "^11.0.0",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
